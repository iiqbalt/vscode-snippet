	"Admin Lte Box Info": {
		"prefix": "boxinfo",
		"body": [
			"<div class=\"box box-info\">",
			"    <div class=\"box-body\">",
			"        ",
			"    </div>",
			"</div>"
		],
		"description": "Admin Lte Box Info"
	},

	"Yii2 Referrer": {
		"prefix": "referrer",
		"body": [
			"<?=Html::a(",
			"    '<i class=\"feather-arrow-left\"></i> Kembali',",
			"     Yii::$$app->request->referrer,",
			"     ['class' => 'btn btn-default']) ?>"
		],
		"description": "Yii2 Referrer"
	}
}
